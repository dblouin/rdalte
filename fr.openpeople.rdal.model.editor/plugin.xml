<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="fr.openpeople.rdal.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="fr.openpeople.rdal.model.core.presentation.CoreModelWizard"
            name="%_UI_CoreModelWizard_label"
            class="fr.openpeople.rdal.model.core.presentation.CoreModelWizard"
            category="fr.openpeople.rdal.Wizard.category.ID"
            icon="icons/full/obj16/CoreModelFile.gif">
         <description>%_UI_CoreModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="fr.openpeople.rdal.model.core.presentation.CoreEditorID"
            name="%_UI_CoreEditor_label"
            icon="icons/full/obj16/CoreModelFile.gif"
            extensions="rdal"
            class="fr.openpeople.rdal.model.core.presentation.CoreEditor"
            contributorClass="fr.openpeople.rdal.model.core.presentation.CoreActionBarContributor">
      </editor>
   </extension>

</plugin>
